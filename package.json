{
	"name": "frontend",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"test:ci": "ng test --watch=false --browsers=ChromeHeadless --no-progress",
		"format": "prettier --write \"src/**/*.{ts,js,html,scss,css,md,json}\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"prepare": "husky",
		"pre-commit": "pnpm run format && pnpm run lint",
		"commit-msg": "pnpm commitlint --edit",
		"pre-push": "pnpm run test:ci"
	},
	"lint-staged": {
		"**/*.{js,ts,json,css,scss,html,md}": [
			"prettier --write"
		]
	},
	"private": true,
	"dependencies": {
		"@angular/common": "^19.2.0",
		"@angular/compiler": "^19.2.0",
		"@angular/core": "^19.2.0",
		"@angular/forms": "^19.2.0",
		"@angular/platform-browser": "^19.2.0",
		"@angular/platform-browser-dynamic": "^19.2.0",
		"@angular/router": "^19.2.0",
		"@primeng/themes": "^19.0.10",
		"lucide-angular": "^0.509.0",
		"primeng": "^19.0.10",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.15.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^19.2.1",
		"@angular-eslint/eslint-plugin": "^19.4.0",
		"@angular-eslint/eslint-plugin-template": "^19.4.0",
		"@angular-eslint/template-parser": "^19.4.0",
		"@angular/cli": "^19.2.1",
		"@angular/compiler-cli": "^19.2.0",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@eslint/js": "^9.26.0",
		"@types/jasmine": "~5.1.0",
		"@typescript-eslint/eslint-plugin": "^8.32.0",
		"@typescript-eslint/parser": "^8.32.0",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.0",
		"globals": "^16.1.0",
		"husky": "^9.1.7",
		"jasmine-core": "~5.6.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.1",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"lint-staged": "^16.0.0",
		"prettier": "3.5.3",
		"typescript": "~5.7.2",
		"typescript-eslint": "^8.32.0"
	}
}
